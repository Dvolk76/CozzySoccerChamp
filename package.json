{
  "name": "cozy-soccer-champ",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:worker": "npx wrangler dev",
    "build": "tsc -p tsconfig.json",
    "build:worker": "tsc -p tsconfig.json && npx wrangler deploy --dry-run",
    "start": "node dist/index.js",
    "deploy": "npx wrangler deploy",
    "prisma:generate": "prisma generate",
    "prisma:generate:d1": "prisma generate --schema=prisma/schema-d1.prisma",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:d1": "npx wrangler d1 migrations apply cozy-soccer-champ-db",
    "prisma:deploy": "prisma migrate deploy",
    "d1:create": "npx wrangler d1 create cozy-soccer-champ-db",
    "d1:migrations": "npx wrangler d1 migrations list cozy-soccer-champ-db",
    "backup": "node scripts/manual-backup.mjs",
    "backup:restore": "node scripts/restore-backup.mjs",
    "backup:setup": "node scripts/auto-setup-backup.mjs",
    "r2:create": "npx wrangler r2 bucket create cozy-soccer-backups",
    "r2:list": "npx wrangler r2 bucket list"
  },
  "dependencies": {
    "@prisma/adapter-d1": "^5.17.0",
    "@prisma/client": "^5.17.0",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.4.5",
    "express": "5.1.0",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "pino": "8.21.0",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240909.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.8",
    "@types/node": "^22.18.4",
    "ngrok": "^5.0.0-beta.2",
    "prisma": "^5.17.0",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4",
    "wrangler": "^4.38.0"
  }
}
